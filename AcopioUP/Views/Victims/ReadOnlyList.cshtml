@using AcopioUP.Models
@model IEnumerable<Victim>
@{
    ViewBag.Title = "ReadOnlyList";
}

<h2>Damnificados</h2>
<table id="victims" class="table table-bordered table-hover" >
    <thead>
    <tr>
        <th>Nombre</th>
        <th>Email</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var victim in Model)
    {
        <tr>
            <td>
                @victim.FullName
            </td>
            <td>
                @victim.Email
            </td>
        </tr>
    }
    </tbody>
</table>

@section scripts
{
    <script>
        $(document).ready(function() {
            $("#victims").DataTable();
        });
    </script>
}